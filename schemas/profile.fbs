
table NdbProfile {
  name:string;
  website:string;
  about:string;
  lud16:string;
  banner:string;
  display_name:string;
  reactions:bool = true;
  picture:string;
  nip05:string;
  damus_donation:int;
  damus_donation_v2:int;
  lud06:string;
}

table NdbProfileRecord {
  profile:NdbProfile;
  received_at:ulong;
  note_key:ulong;

  // artifacts
  lnurl:string;
}

root_type NdbProfileRecord;

